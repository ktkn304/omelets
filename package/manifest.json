{
    "name": "omelets",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "128": "img/omelet_128.png"
    },
    "description": "bookmarklet editor",
    "action": {
        "default_popup": "./popup.html",
        "default_icon": {
            "19": "img/omelet_19.png"
        }
    },
    "options_page": "./options.html",
    "content_scripts": [
    ],
    "permissions": [
        "activeTab",
        "bookmarks",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}